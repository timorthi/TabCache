{
    "manifest_version": 2,
    "author": "Timothy Ng",

    "name": "TabCache Session Manager",
    "short_name": "TabCache",
    "description": "Keep track of your browsing sessions.",
    "version": "0.1",

    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html",
        "default_title": "TabCache"
    },

    "permissions": [
        "tabs",
        "unlimitedStorage"
    ]
}
